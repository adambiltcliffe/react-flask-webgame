[MASTER]

load-plugins=pylint_mongoengine

[MESSAGES CONTROL]

disable=all

# Same as VS Code defaults for now
enable=F,
      E,
      unreachable,
      duplicate-key,
      unnecessary-semicolon,
      global-variable-not-assigned,
      unused-variable,
      binary-op-exception,
      bad-format-string,
      anomalous-backslash-in-string,
      bad-open-mode
